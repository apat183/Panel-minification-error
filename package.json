{
  "name": "audit_lead",
  "description": "AuditLead sample for Google App Engine",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "deploy": "gcloud app deploy",
    "node-start": "node ./bin/www",
    "node-dev": "set NODE_ENV=development&& nodemon --debug ./bin/www",
    "node-dev-mac": "NODE_ENV=development nodemon --debug ./bin/www",
    "node-prod": "set NODE_ENV=production&& nodemon --debug ./bin/www",
    "node-dev-prod": "NODE_ENV=production nodemon --debug ./bin/www",
    "mac-dev": "npm run node-dev-mac & npm run mac-bsync",
    "win-dev": "start npm run node-dev && start npm run win-bsync",
    "mac-prod": "npm run node-prod & npm run mac-bsync",
    "win-prod": "start npm run node-prod && start npm run win-bsync",
    "mac-bsync": "browser-sync start -f 'app' --port '4000' -p 'https://localhost:3000'",
    "win-bsync": "browser-sync start -f \"app\" --port \"4000\" -p \"https://localhost:3000\""
  },
  "engines": {
    "node": "~4.2"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "aws-sdk": "^2.2.36",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "cryptr": "^2.0.0",
    "debug": "~2.2.0",
    "ejs": "^2.4.2",
    "express": "~4.13.1",
    "firebase": "^3.0.3",
    "gcloud": "^0.36.0",
    "grunt": "~0.4.5",
    "grunt-autoprefixer": "~2.0.0",
    "grunt-concurrent": "~1.0.0",
    "grunt-contrib-clean": "~0.6.0",
    "grunt-contrib-concat": "~0.5.0",
    "grunt-contrib-connect": "~0.9.0",
    "grunt-contrib-copy": "~0.7.0",
    "grunt-contrib-cssmin": "~0.12.0",
    "grunt-contrib-htmlmin": "~0.4.0",
    "grunt-contrib-imagemin": "~1.0.0",
    "grunt-contrib-jshint": "~0.11.0",
    "grunt-contrib-uglify": "~0.11.1",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-filerev": "~2.1.2",
    "grunt-google-cdn": "~0.4.3",
    "grunt-newer": "~1.1.0",
    "grunt-ng-annotate": "~0.10.0",
    "grunt-svgmin": "~2.0.0",
    "grunt-usemin": "~3.0.0",
    "grunt-wiredep": "~2.0.0",
    "imagemin": "~4.0.0",
    "jade": "~1.11.0",
    "jshint-stylish": "~1.0.0",
    "jsonwebtoken": "~5.5.4",
    "load-grunt-tasks": "~3.1.0",
    "mkdirp": "^0.5.1",
    "moment": "~2.11.2",
    "morgan": "~1.6.1",
    "q": "~1.4.1",
    "randomstring": "~1.1.4",
    "request": "~2.69.0",
    "rimraf": "^2.5.2",
    "sendgrid": "^3.0.5",
    "serve-favicon": "~2.3.0",
    "vinyl-fs": "~2.3.1"
  }
}
